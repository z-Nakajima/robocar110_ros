<launch>
  <node pkg="joy" type="joy_node" name="joy_node"/>

  <arg name="joy_msg" default="/joy"/>

  <node pkg="rc110_teleop" type="rc110_joy_teleop" name="rc110_joy_teleop" output="screen">
    <remap from="~input_joy" to="$(arg joy_msg)"/>
  </node>

  <include file="$(find rc110_launch)/launch/all.launch">
    <remap from="/drive" to="/rc110_joy_teleop/output_cmd" />
  </include>
</launch>
